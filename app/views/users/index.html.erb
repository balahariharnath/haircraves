<p style="color: green"><%= notice %></p>

<h1>Users</h1>

<div id="users">
  <% @users.each do |user| %>
    <%= render user %>
    <p>
      <%= link_to "Show this user", user %>
    </p>
  <% end %>
</div>
<%= button_to "sign out", destroy_user_session_path, method: :delete %>
<%= link_to "New user", new_user_path %>
